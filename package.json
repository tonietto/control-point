    "semantic-ui-react": "^0.68.4",
    "stylus": "^0.54.5"
  },
  "scripts": {
    "start": "concurrently -k -r \"react-scripts start\" \"yarn run styles\"",
    "staging": "yarn run build && cd build && cp index.html 200.html && surge . control-point.surge.sh",
    "deploy-storybook": "build-storybook -c .storybook -o .out && cd .out && cp index.html 200.html && surge . control-point-storybook.surge.sh",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "storybook": "concurrently -k -r \"start-storybook -p 9009 -s public\" \"yarn run styles\"",
    "build-storybook": "build-storybook -s public",
    "styles": "stylus -w stylus/styles.styl -o src/assets/bundled-styles.css"
  }
}
