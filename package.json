  "dependencies": {
    "apollo-client": "^1.3.0",
    "flow-bin": "^0.50.0",
    "graphql-tag": "^2.2.1",
    "material-ui": "^0.18.1",
    "react": "^15.3.2",
    "react-apollo": "^1.2.0",
    "react-dom": "^15.3.2",
    "react-intl": "^2.2.3",
    "react-intl-redux": "^0.5.0",
    "react-lazyload": "^2.1.6",
    "react-motion": "^0.5.0",
    "react-motion-ui-pack": "^0.10.2",
    "react-redux": "^5.0.1",
    "react-router": "^4.1.1",
    "react-router-redux": "^4.0.7",
    "react-tap-event-plugin": "^2.0.1",
